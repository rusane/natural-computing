    "# Source of PSO package: https://github.com/tisimst/pyswarm\n",
    "from pyswarm import pso\n",
    "\n",
    "def f(x):\n",
    "    return x**2\n",
    "\n",
    "lb = [-10]\n",
    "ub = [10]\n",
    "\n",
    "xopt, fopt = pso(f, lb, ub, swarmsize=1, omega=0.5, debug=True)\n",
    "\n",
    "xopt\n",
    "\n",
    "# Optimum should be x=[0.0] with f(x)=0.0"
